  trialsName: 11 - Sleight Cancel
  trialsDescription: Learn how to play Sleight without getting Card Break.
  deckPlayer: {fileID: 11400000, guid: d6d5007736698ce438d8375880778f5b, type: 2}
  deckEnemy: {fileID: 11400000, guid: 1ab145676a20364469b068469819d2e5, type: 2}
